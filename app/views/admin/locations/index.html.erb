<table>
  <th>IP</th>
  <th>User Agent</th>
  <th>Date</th>
  <th></th>
  <% @locations.each do |location| %>
    <tr>
      <td><%= location.ip %></td>
      <td><%= location.user_agent %></td>
      <td><%= Time.zone.parse(location.created_at.to_s).localtime %></td>
      <td>
        <%= link_to 'Delete', admin_location_path(location), method: :delete %>
      </td>
    </tr>
  <% end %>
</ul>
